{
  "name": "tpl-electron-webpack-typescript-vue",
  "description": "This project is a working template for Electron, Webpack, Typescript and VueJS",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "electron",
    "typescript",
    "webpack",
    "vuejs"
  ],
  "scripts": {
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "dependencies": {
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.20.4",
    "electron-webpack": "^2.1.2",
    "ts-loader": "^4.4.2",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.0"
  },
  "resolutions": {
    "webpack-sources": "1.1.0"
  }
}
